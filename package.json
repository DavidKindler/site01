{
  "name": "gatsby-starter-default",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "autoprefixer": "^9.5.0",
    "core-js": "2",
    "cssnano": "^4.1.10",
    "gatsby": "^2.2.5",
    "gatsby-image": "^2.0.34",
    "gatsby-plugin-manifest": "^2.0.24",
    "gatsby-plugin-offline": "^2.0.25",
    "gatsby-plugin-react-helmet": "^3.0.11",
    "gatsby-plugin-sass": "^2.0.11",
    "gatsby-plugin-sharp": "^2.0.30",
    "gatsby-remark-prismjs": "^3.2.6",
    "gatsby-source-filesystem": "^2.0.27",
    "gatsby-transformer-remark": "^2.3.8",
    "gatsby-transformer-sharp": "^2.1.17",
    "postcss": "^7.0.14",
    "postcss-cli": "^6.1.2",
    "prism": "^4.1.2",
    "prismjs": "^1.16.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-helmet": "^5.2.0"
  },
  "devDependencies": {
    "@emotion/core": "^10.0.9",
    "@emotion/styled": "^10.0.9",
    "eslint-config-standard-jsx": "^6.0.2",
    "gatsby-plugin-emotion": "^4.0.6",
    "lodash": "^4.17.11",
    "node-sass": "^4.11.0",
    "prettier": "^1.16.4",
    "react-syntax-highlighter": "^10.2.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf .cache/",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\"",
    "buildcss": "node-sass ./sass/nxp-web.scss  -o ./assets/css",
    "node-sass": "node-sass",
    "postcss-build": "postcss --use autoprefixer -c postcss-options.json ./assets/css/nxp-web.css -o ./assets/css/nxp-web.min.css",
    "postcss": "postcss"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
